#!/bin/bash
#
#YLESANNE4
grupis=reisijatearv
kohad=vabadkohad
echo "Mitu reisijat on grupis: "
read reisijatearv
echo "Mitu kohta on 체hes bussis: "
read vabadkohad
taisbusse=$(( vabadkohad / reisijatearv  ))
buss_ymarda="`echo "($taisbusse+0.9)/1" | bc`" 
mahajaanud=$((reisijatearv - vabadkohad))
echo "T채is busse on $buss_ymarda ja maha on j채채nud $mahajaanud" 
